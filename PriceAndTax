#include <iostream>
using namespace std;

// Declare function here


float AddTax(float dollar)
{
	float AddTax = dollar * .12 + dollar;
	return AddTax;
}

int main()
{
	cout << "PRICE PLUS TAX" << endl << endl;

	cout << "Price: $" << 9.99
		<< " \t with tax: $" << AddTax(9.99) << endl;

	cout << "Price: $" << 19.95
		<< " \t with tax: $" << AddTax(19.95) << endl;

	cout << "Price: $" << 10.00
		<< " \t with tax: $" << AddTax(10.00) << endl;

	// Don't quit automatically
	cin.ignore();
	cin.get();
	return 0;
}
